{
  "name": "prueba",
  "productName": "IVa DIGITAL",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icono.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Agustin Lorenzatto",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.2.1"
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "browserify-fs": "^1.0.0",
    "xlsx": "^0.17.0"
  }
}
